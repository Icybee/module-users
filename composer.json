{
	"name": "icybee/module-users",
	"type": "icanboogie-module",
	"description": "Manages the users of the CMS Icybee",
	"keywords": [ "user", "login" ],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/module-users/issues",
		"source": "https://github.com/Icybee/module-users"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4",
		"icanboogie/module-installer": "*",
		"icanboogie/activerecord": "2.x",
		"icanboogie/event": "*",
		"brickrouge/css-class-names": "2.x",
		"icybee/core": "2.x",
		"ircmaxell/password-compat": "1.x"
	},
	"require-dev": {
		"icanboogie/operation": "2.x",
		"icanboogie/icanboogie": "2.x"
	},
	"suggest": {
		"icanboogie/mailer": "Mailer is a library for PHP that is designed to handle emails generation and sending."
	},
	"autoload": {
		"classmap": [ "lib" ]
	},
	"scripts": {
		"post-autoload-dump": "ICanBoogie\\AutoConfig\\Generator::on_autoload_dump"
	}
}